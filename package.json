  "scripts": {
    "prestart": "yarn sync",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --write src",
    "lint": "eslint .",
    "subinit": "git submodule init && git submodule update --remote --merge",
